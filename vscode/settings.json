	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"keyword",
					"keyword.control",
					"keyword.operator.new",
					"keyword.operator.cast",
					"keyword.other.operator",
					"keyword.operator.typeid",
					"keyword.operator.sizeof",
					"keyword.operator.delete",
					"keyword.control.directive",
					"keyword.operator.expression",
					"storage.type",
					"storage.modifier.const",
					"storage.modifier.static",
					"storage.modifier.virtual",
					"storage.modifier.specifier.const",
					"storage.modifier.specifier.static",
					"storage.modifier.specifier.functional.post-parameters.const",
					"storage.modifier.specifier.functional.pre-parameters.virtual",
					"variable.language.this",
					"entity.name.function.preprocessor",
					"meta.preprocessor",
					"meta.function.definition.parameters.special.member.destructor",
				],
				"settings": {
					"foreground": "#f799ffde",
				}
			},
			{
				"scope": [
					"source.cpp",
					"storage.modifier",
					"entity.name.type",
					"entity.name.function",
					"entity.name.namespace",
					"entity.name.scope-resolution",
					"variable.parameter",
					"variable.other.local",
					"variable.other.object",
					"variable.other.property",
					"variable.other.enummember",
					"meta.block",
					"meta.function",
					"meta.body.class",
					"meta.embedded",
					"keyword.operator",
					"punctuation.definition.template-expression.begin",
					"punctuation.definition.template-expression.end",
				],
				"settings": {
					"foreground": "#b4b3b3",
				}
			},
			{
				"scope": [
					"constant.numeric",
					"constant.language",
					"keyword.other.unit.suffix.floating-point",
				],
				"settings": {
					"foreground": "#f89a27da",
				}
			},
			{
				"scope": "string",
				"settings": {
					"foreground": "#9fff98db",
				}
			},
			{
				"scope": "constant.character.escape",
				"settings": {
					"foreground": "#69e9ffb3",
				}
			},
			{
				"scope": "comment",
				"settings": {
					"foreground": "#b4b3b3a3",
				}
			},
			{
				"scope": [
					"variable.other.makefile",
					"variable.language.makefile",
					"punctuation.definition.variable.makefile",
					"string.interpolated.makefile",
				],
				"settings": {
					"foreground": "#ff67abd6",
				}
			},
			{
				"scope": [
					"support.function.addprefix.makefile",
					"support.function.addsuffix.makefile",
					"support.function.target.PHONY.makefile",
				],
				"settings": {
					"foreground": "#ff99f0d6",
				}
			},
			{
				"scope": "meta.scope.recipe.makefile",
				"settings": {
					"foreground": "#ff9a56d6",
				}
			},
			{
				"scope": "entity.name.function.target.makefile",
				"settings": {
					"foreground": "#56a5ffd6",
				}
			},
			{
				"scope": "source.makefile",
				"settings": {
					"foreground": "#b4b3b3",
				},
			},
			{
				"scope": "constant.language.module.http.core",
				"settings": {
					"foreground": "#f799ffde",
				},
			},
			{
				"scope": "constant.language.module.http.index",
				"settings": {
					"foreground": "#9fff98db",
				},
			},
			{
				"scope": "source.nginx",
				"settings": {
					"foreground": "#b4b3b3",
				},
			},
			{
				"scope": [
					"entity.name.tag.html",
				],
				"settings": {
					"foreground": "#fb6a9de9",
				},
			},
			{
				"scope": [
					"entity.other.attribute-name.html",
					"support.type.property-name.html",
					"entity.other.attribute-name.css",
					"support.type.property-name.css",
					"support.type.property-name.media.css",
					"entity.other.attribute-name.class.css",
					"entity.other.attribute-name.id.css",
					"entity.name.tag.css",
					"variable.other.readwrite.js",
					"variable.other.constant.js",
					"meta.brace.round.js",
					"punctuation.definition.block.js",
					"meta.object-literal.key.js",
					"punctuation.separator.comma.js",
					"punctuation.terminator.statement.js",
					"punctuation.accessor.js",
				],
				"settings": {
					"foreground": "#b4b3b3",
				},
			},
			{
				"scope": "string.quoted.double.html",
				"settings": {
					"foreground": "#9fff98db",
				},
			},
			{
				"scope": [
					"text.html.derivative",
					"meta.property-list.css",
					"punctuation.definition.tag",
					"punctuation.definition.parameters",
					"punctuation.section.media",
					"keyword.control.at-rule"
				],
				"settings": {
					"foreground": "#b4b3b3",
				},
			},
			{
				"scope": [
					"support.constant.color",
					"support.constant.property-value.css",
					"keyword.other.unit",
				],
				"settings": {
					"foreground": "#ff9a56d6",
				},
			},
		]
	},
